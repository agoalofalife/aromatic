{
  "compileOnSave": true,
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "moduleResolution": "node",
  "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "outDir": "ts-build", "jsx": "preserve"
  },
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "test",
    "typings"
  ]
}